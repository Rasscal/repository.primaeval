  <import addon="script.module.requests" version="2.9.1" />
</requires>
<extension point="xbmc.python.pluginsource" library="main.py">
  <provides>video</provides>
</extension>
<extension point="xbmc.addon.metadata">
  <summary lang="en">TV Listings (UK Only so far)</summary>
  <description lang="en">TV Listings (UK Only so far). Meta4Kodi catchup playback. Addon ini files for live stream selection.</description>
